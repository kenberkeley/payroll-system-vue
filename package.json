{
  "name": "payroll-system-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "pretest": "npm run lint",
    "test": "npm run test:unit && npm run test:e2e -- --headless",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "lint:api": "standard api/**/*.js",
    "dev:api": "nodemon --exec npm run start:api",
    "start:api": "node -r esm api/main.js",
    "prestart:api": "npm run lint:api",
    "dev": "run-p serve dev:api",
    "postinstall": "rimraf snapshots.js && link-module-alias"
  },
  "_moduleAliases": {
    "~": "./"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bulma": "^0.7.5",
    "bulma-toast": "^1.5.1",
    "core-js": "^2.6.5",
    "dayjs": "^1.8.14",
    "detect-port": "^1.3.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "http-status-codes": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "lowdb": "^1.0.0",
    "shortid": "^2.2.14",
    "vue": "^2.6.10",
    "vue-meta": "^2.0.3",
    "vue-router": "^3.0.3",
    "vuelidate": "^0.7.4",
    "vuex": "^3.0.1",
    "vuex-map-fields": "^1.3.4"
